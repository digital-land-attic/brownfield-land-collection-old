function ldsCloseGetInvolved () {
  var date = new Date();
  date.setTime(date.getTime()+(48*60*60*1000));
  document.cookie = 'lds_hide_get_involved=true; domain=.' + window.location.host.toString() + '; path=/; expires=' + date.toGMTString();
  document.body.classList.add('lds-close-get-involved')
  return false;
}

function ldsCheckGetInvolved(name) {
 return !!document.cookie.match(new RegExp('lds_hide_get_involved' + '=([^;]+)'));
}

try {
  if (ldsCheckGetInvolved()) {
    document.body.classList.add('lds-close-get-involved')
  }
} catch (e) {
  console.error(e)
}
</script>

